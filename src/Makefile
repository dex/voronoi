####### Implicit rules

.SUFFIXES: .c .o .cpp .cc .cxx .C

.cpp.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cc.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cxx.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.C.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.c.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o $@ $<

####### Build rules

all: Makefile $(TARGET)

$(TARGET):  $(UICDECLS) $(OBJECTS) $(OBJMOC)  
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(OBJCOMP) $(LIBS)

mocables: $(SRCMOC)
uicables: $(UICDECLS) $(UICIMPLS)

$(MOC): 
	( cd $(QTDIR)/src/moc && $(MAKE) )

Makefile: voronoi.pro  /usr/share/qt3/mkspecs/default/qmake.conf /usr/share/qt3/lib/libqt-mt.prl
	$(QMAKE) -o Makefile voronoi.pro
qmake: 
	@$(QMAKE) -o Makefile voronoi.pro

dist: 
	@mkdir -p .tmp/voronoi && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .tmp/voronoi/ && $(COPY_FILE) --parents inputdialog.ui.h .tmp/voronoi/ && ( cd `dirname .tmp/voronoi` && $(TAR) voronoi.tar voronoi && $(GZIP) voronoi.tar ) && $(MOVE) `dirname .tmp/voronoi`/voronoi.tar.gz . && $(DEL_FILE) -r .tmp/voronoi

mocclean:
	-$(DEL_FILE) $(OBJMOC)
	-$(DEL_FILE) $(SRCMOC)

uiclean:
	-$(DEL_FILE) $(UICIMPLS) $(UICDECLS)

yaccclean:
lexclean:
clean: mocclean uiclean
	-$(DEL_FILE) $(OBJECTS)
	-$(DEL_FILE) *~ core *.core


####### Sub-libraries

distclean: clean
	-$(DEL_FILE) $(TARGET) $(TARGET)


FORCE:

####### Compile

algorithm.o: algorithm.cpp geometry.h \
		algorithm.h \
		convex.h

convex.o: convex.cpp geometry.h \
		convex.h

geometry.o: geometry.cpp geometry.h \
		tooltip.h \
		algorithm.h

main.o: main.cpp mainwindow.h \
		geometry.h

mainwindow.o: mainwindow.cpp mainwindow.h \
		geometry.h \
		inputdialog.h

tooltip.o: tooltip.cpp tooltip.h \
		geometry.h

inputdialog.h: inputdialog.ui 
	$(UIC) inputdialog.ui -o inputdialog.h

inputdialog.cpp: inputdialog.h inputdialog.ui inputdialog.ui.h 
	$(UIC) inputdialog.ui -i inputdialog.h -o inputdialog.cpp

inputdialog.o: inputdialog.cpp inputdialog.ui.h \
		inputdialog.h

moc_geometry.o: moc_geometry.cpp  geometry.h 

moc_mainwindow.o: moc_mainwindow.cpp  mainwindow.h 

moc_inputdialog.o: moc_inputdialog.cpp  inputdialog.h 

moc_geometry.cpp: $(MOC) geometry.h
	$(MOC) geometry.h -o moc_geometry.cpp

moc_mainwindow.cpp: $(MOC) mainwindow.h
	$(MOC) mainwindow.h -o moc_mainwindow.cpp

moc_inputdialog.cpp: $(MOC) inputdialog.h
	$(MOC) inputdialog.h -o moc_inputdialog.cpp

